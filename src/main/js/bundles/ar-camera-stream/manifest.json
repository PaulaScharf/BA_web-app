{
    "name": "ar-camera-stream",
    "title": "Camera Bundle",
    "version": "1.0.22",
    "main": "",
    "i18n": [],
    "dependencies": {
        "apprt-core": "4.7.0"
    },
    "CSS-Themes-Extension": [{
            "name": "*",
            "files": ["styles/styles.css"]
        }],
    "layout-widgets": [{
            "widgetRole": "camera",
            "template": "seasons",
            "attachTo": "mapPlaceHolder"
        }],
    "components": [{
            "name": "Config",
            "impl": "./CameraWidgetFactory",
            "provides": ["dijit.Widget"],
            "instanceFactory": true,
            "properties": {
                "widgetRole": "camera"
            }
        }]
}