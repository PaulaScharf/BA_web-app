{
    "name": "ar-layout-template-resizer",
    "title": "Layout Template Resizer",
    "description": "This bundle resizes the view to a 1:1 ratio",
    "version": "0.0.1",
    "dependencies": {
        "ar-config": "0.0.9",
        "map-widget": "4.8.4"
    },
    "CSS-Themes-Extension": [
        {
            "name": "*",
            "files": [
                "styles/styles.css"
            ]
        }
    ],
    "main": "",
    "i18n": [],
    "components": [{
            "name": "TemplateResizer",
            "properties": {},
            "references": [{
                    "name": "config",
                    "providing": "ar-config.Config"
                }, {
                    "name": "mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                }, {
                    "name": "camera",
                    "providing": "dijit.Widget",
                    "filter": "(widgetRole=camera)"
                }]
        }]
}