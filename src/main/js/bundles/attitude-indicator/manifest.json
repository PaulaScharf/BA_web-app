{
    "name": "attitude-indicator",
    "title": "attitude indicator bundle",
    "version": "1.0.0",
    "dependencies": {
        "apprt-vue": "^4.7.2",
        "apprt-vuetify": "^4.7.2",
        "apprt-core": "^4.7.2",
        "apprt-binding": "^4.7.2",
        "ar-device-data-provider": "^0.0.9"
    },
    "main": "",
    "i18n": [],
    "layout-widgets": [{
        "widgetRole": "attitudeIndicator",
        "window": {
            "show": "true"
        }
    }],
    "components": [{
        "name": "horizonFactory",
        "provides": ["dijit.Widget"],
        "instanceFactory": true,
        "properties": {
            "widgetRole": "attitudeIndicator"
        },
        "references": [{
            "name": "deviceData",
            "providing": "ar-device-data-provider.DeviceData"
        }]
    }]
}